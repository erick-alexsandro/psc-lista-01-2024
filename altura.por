programa {
  funcao inicio() {
    escreva("Escreva seu nome:")
    cadeia nome
    leia(nome)
    escreva("Escreva sua idade:")
    inteiro idade
    leia(idade)
    escreva("Escreva sua altura:")
    real altura
    leia(altura)
    escreva("Seu nome é " + nome + ". Você tem " + idade + " anos e tem " + altura + " m.")
  }
}
